#include<stdio.h>
int main()
{
	int i, n, m, j;
	long int a[100];
	int b[100][2];
	int c[100];
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		scanf("%11d%d%d",&a[i],&b[i][0],&b[i][1]);
	}
	scanf("%d", &m);
	for(i=0;i<m;i++)
	{
		scanf("%d",&c[i]);
	}
	for(i=0;i<m;i++)
	{
		for (j=0;j<n;j++)
		{
			if(c[i]==b[j][0])
			{
				printf("%11d%d\n",a[j],b[j][1]);
				break;
			}
		}
	}
	return 0;
}
